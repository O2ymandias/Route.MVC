@model UserVM
@{
    ViewData["Title"] = "Details";
}
<h1>The Details Of <b>@string.Concat(Model.FirstName, " ", Model.LastName)</b> </h1>
<div class="mt-3">
	<partial name="PartialViews/DeleteDetailsPartial" model="@Model" />
	<br />
	<div>
		<a class=" btn btn-success" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>|
		<a class=" btn btn-light" asp-action="Index">Back To List</a>
	</div>
</div>

