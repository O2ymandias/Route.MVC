@model string
<td>
	<a class="text-info" asp-action="Details" asp-route-id="@Model">
		<i class="fa-solid fa-eye"></i>
	</a>
</td>

<td>
	<a class="text-primary" asp-action="Edit" asp-route-id="@Model">
		<i class="fa-solid fa-pen-to-square"></i>
	</a>
</td>

<td>
	<a class="text-danger" asp-action="Delete" asp-route-id="@Model">
		<i class="fa-solid fa-trash"></i>
	</a>
</td>
